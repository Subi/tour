// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Patch {
  id Int @id @default(autoincrement())
  state String?
  date String
  image_url String?
  isApproved Boolean
  Author String?
}

